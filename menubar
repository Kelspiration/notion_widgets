	<script>

		let direction = "horizontal";
		if (direction === "vertical") {
			document.head.insertAdjacentHTML("beforeend", `<style>.navMenu a {display: block; text-align:left; margin:7px} i{width:15px; margin-right:5px} .navMenu{margin:3px}</style>`)
		}


		function navigate(id) {

			if (id == 'home')
				window.open("https://www.notion.so/kelspirationn/IT-S-GIVING-SLAY-0527ec6f54984b4682d62be514b23c57?pvs=4", '_blank').focus();
			else if (id == "tasks")
				window.open("https://www.notion.so/kelspirationn/TASKS-a04b2f4979694de5b1af20aaa4b28049?pvs=4", '_blank').focus();
			else if (id == "days")
				window.open("https://www.notion.so/kelspirationn/DAYS-9055a5f5f55b4bf3840171fd1ec36c9c?pvs=4", '_blank').focus();
			else if (id == "weeks")
				window.open("https://www.notion.so/kelspirationn/WEEKS-3c188d08948d4aa9b50e848dfdc3e541?pvs=4", '_blank').focus();
			else if (id == "my whys")
				window.open("https://www.notion.so/kelspirationn/MY-WHYS-705c6b96f8824b0691df4d0a09e839c2?pvs=4", '_blank').focus();
			else if (id == "idea chamber")
				window.open("https://www.notion.so/kelspirationn/IDEA-CHAMBER-514924953b09440a92dd97675721ce92?pvs=4", '_blank').focus();
			else if (id == "shop journal")
				window.open("https://www.notion.so/kelspirationn/SHOP-JOURNAL-0714048900f44e0fb2a77f5bf758c621?pvs=4", '_blank').focus();
			else if (id == "book writing")
				window.open("https://www.notion.so/kelspirationn/BOOK-WRITING-f0be9c9a12384c8ba999ca4c0daaef10?pvs=4", '_blank').focus();
			else if (id == "socials")
				window.open("https://www.notion.so/kelspirationn/SOCIALS-39ff6102113449e9bc11172079f50e4a?pvs=4", '_blank').focus();
		

		}
	</script>


